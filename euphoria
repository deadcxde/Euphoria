            function isIslandNearest()
                islands = {"Island 5", "Island 4", "Island 3", "Island 2", "Island 1"}
                for i,islandName in pairs(islands) do
                    for i,v in pairs(game.Workspace._WorldOrigin.Locations:GetChildren()) do
                        if v.Name == islandName then
                            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).Magnitude <= 5000 then
                                return true
                            end
                        end
                    end
                end
                return false
            end
            task.spawn(function()
                while task.wait() do
                    pcall(function()
                        if getgenv().Settings["autonextisland"] then
                            islands = {"Island 5", "Island 4", "Island 3", "Island 2", "Island 1"}
                            if game.Players.LocalPlayer.PlayerGui.Main.Timer.Visible == true then
                                NoClip = true
                                found = false
                                for i,islandName in pairs(islands) do
                                    for i,v in pairs(game.Workspace._WorldOrigin.Locations:GetChildren()) do
                                        if v.Name == islandName then
                                            if (game.Players.LocalPlayer.Character.HumanoidRootPart.Position - v.Position).Magnitude <= 5000 then
                                                toPoint(CFrame.new(v.CFrame.X, 45, v.CFrame.Z))
                                                found = true
                                                break
                                            end
                                        end
                                    end
                                    if found then break end
                                end
                            else
                                NoClip = false
                            end
                        else
                            NoClip = false
                        end
                    end)
                end
            end)
